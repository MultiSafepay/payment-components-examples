{
    "name": "itctest/test",
    "type": "project",
    "description": "ITC test",    
    "license": "MIT",
    "require": {
        "php": "^8",
        "guzzlehttp/guzzle": "^6.4",
        "vlucas/phpdotenv": "^3.6"
    },
    "require-dev": {
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
     "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        
    }
}
